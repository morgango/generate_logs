# Log Generator Configuration
# Output directory and file
LOG_OUTPUT=/tmp/loadgen/loadgen.log

# Number of lines per format (default: 5000)
LOG_LINES=3

# Log format (apache, json, csv, pipe, kv, hadoop, all)
LOG_FORMAT=all

# Duration in seconds for time-based execution
LOG_DURATION=60

# Pause duration in seconds between iterations
LOG_PAUSE=.5
