# Log Generator Configuration
# Copy this file to .env and modify the values as needed

# Number of lines to generate per format (default: 5000)
LOG_LINES=1000

# Output file path (leave empty for auto-detect)
LOG_OUTPUT=/tmp/generated_logs.log

# Log format to generate: apache, json, csv, pipe, kv, hadoop, all (default: all)
LOG_FORMAT=all

# Duration in seconds for time-based execution (leave empty to run once)
# LOG_DURATION=60
